<?xml version="1.0"?>
<package >
  <metadata>
    <id>IEIT.Reports.Export.Helpers</id>
	
	<version>0.4.0-debug</version>
	
    <title>Excel Helpers</title>
    <authors>sultan.imbayev, yerlanyr</authors>
    <owners>IPEIT KBTU</owners>
    <projectUrl>https://github.com/sultanimbayev/IEIT.Reports.Export.Helpers</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
        Shortcut extensions for DocumentFormat.OpenXml.Spreadsheet classes
        Расширения-шорткоды для классов из библиотеки DocumentFormat.OpenXml.Spreadsheet
	</description>
    <summary>
        Даннный пакет предназначен для упрощения синтаксиса библиотеки DocumentFormat.OpenXml.
		This package is to simplify work with Excel Spreadsheet document sing DocumentFormat.OpenXml library.
    </summary>
    <releaseNotes>
		ColumNum -> ColumnNum typo fix; 
		Full styles support: styles now can be copied from another workbook;
	</releaseNotes>
    <tags>OpenXml Excel extension</tags>
	<dependencies>
		<dependency id="DocumentFormat.OpenXml" version="2.5.0"/>
	</dependencies>
	<frameworkAssemblies>
	  <frameworkAssembly assemblyName="System.Configuration" />
	</frameworkAssemblies>
  </metadata>
  <files>
	<file src="..\IEIT.Reports.Export.Helpers\bin\Debug\IEIT.Reports.Export.Helpers.dll" target="lib\net45" />
	<file src="..\IEIT.Reports.Export.Helpers\bin\Debug\IEIT.Reports.Export.Helpers.xml" target="lib\net45" />
	<file src="..\IEIT.Reports.Export.Helpers\bin\Debug\IEIT.Reports.Export.Helpers.pdb" target="lib\net45" />
	<file src="..\IEIT.Reports.Export.Helpers\OpenXML.Elements\**" target="lib\net45\OpenXML.Elements" />
  </files>
</package>